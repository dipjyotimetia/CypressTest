{
  "name": "cypress_test_framework",
  "private": true,
  "description": "Cypress test Framework",
  "author": "Dipjyoti Metia",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf cypress/reports",
    "lint": "tslint ./src/**/*.js -t verbose",
    "combine-reports": "mochawesome-merge ./cypress/reports/mocha/*.json > mochareports/mochawesome.json",
    "generate-report": "marge mochareports/mochawesome.json",
    "cypress:open": "cypress open",
    "start:ci": "my-server -p 3030",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run --headed --spec 'cypress/integration/example_spec.ts'",
    "firefox:run": "cypress run --headed --browser firefox --spec 'cypress/integration/example_spec.ts'",
    "chrome:run": "cypress run --headed --browser chrome --spec 'cypress/integration/example_spec.ts'",
    "edge:run": "cypress run --headed --browser edge --spec 'cypress/integration/example_spec.ts'",
    "cy:ci": "cypress run --spec 'cypress/integration/example_spec.ts'",
    "test:ci": "start-server-and-test start:ci http://localhost:3030 cy:run"
  },
  "keywords": [
    "node",
    "cypress",
    "test",
    "typescript"
  ],
  "dependencies": {
    "axe-core": "^4.2.0",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^7.2.0",
    "cypress-axe": "^0.12.2",
    "cypress-downloadfile": "^1.2.0",
    "date-fns": "^2.21.2",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cypress/skip-test": "^2.6.0",
    "@neuralegion/cypress-har-generator": "^5.2.2",
    "@percy/cli": "1.0.0-beta.48",
    "@percy/cypress": "^3.1.0",
    "@testing-library/cypress": "^7.0.6",
    "@types/chai": "^4.2.17",
    "@types/jquery": "^3.5.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.0",
    "concurrently": "^6.0.2",
    "cypress-dark": "^1.7.14",
    "cypress-file-upload": "^5.0.6",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-ntlm-auth": "^3.2.0",
    "cypress-parallel": "^0.3.0",
    "cypress-xpath": "^1.6.2",
    "eslint-plugin-cypress": "^2.11.2",
    "fs-extra": "^9.1.0",
    "knex": "^0.95.4",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "start-server-and-test": "^1.12.1",
    "typescript": "^4.2.4"
  }
}
