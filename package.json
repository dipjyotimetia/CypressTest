{
  "name": "cypress_test_framework",
  "version": "3.0.0",
  "private": true,
  "description": "Cypress test Framework",
  "scripts": {
    "clean": "rimraf cypress/reports",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "combine-reports": "mochawesome-merge --reportDir cypress/reports/mocha > mochareports/report.json",
    "generate-report": "marge mochareports/*.json -f report -o mochareports",
    "cypress:open": "cypress open",
    "start:ci": "my-server -p 3030",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run --headed --spec 'cypress/integration/sample_spec.js'",
    "firefox:run": "cypress run --headed --browser firefox --spec 'cypress/integration/sample_spec.js'",
    "chrome:run": "cypress run --headed --browser chrome --spec 'cypress/integration/sample_spec.js'",
    "edge:run": "cypress run --headed --browser edge --spec 'cypress/integration/sample_spec.js'",
    "cy:ci": "cypress run --spec 'cypress/integration/example_spec.js'",
    "test:ci": "start-server-and-test start:ci http://localhost:3030 cy:run"
  },
  "keywords": [
    "node",
    "cypress",
    "test",
    "javascript"
  ],
  "author": "Dipjyoti Metia",
  "license": "MIT",
  "dependencies": {
    "cypress": "^4.3.0",
    "bluebird": "^3.7.2",
    "chalk": "3.0.0",
    "cross-env": "7.0.2",
    "faker": "4.1.0",
    "lodash": "^4.17.15",
    "date-fns": "^2.11.0",
    "uuid": "7.0.2",
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "@cypress/skip-test": "^2.5.0",
    "@neuralegion/cypress-har-generator": "^4.0.0",
    "@testing-library/cypress": "^6.0.0",
    "concurrently": "^5.1.0",
    "cypress-dark": "^1.7.14",
    "cypress-file-upload": "^4.0.4",
    "cypress-multi-reporters": "1.2.4",
    "cypress-ntlm-auth": "^2.2.5",
    "cypress-plugin-retries": "^1.5.2",
    "cypress-xpath": "^1.4.0",
    "eslint-plugin-cypress": "^2.10.3",
    "fs-extra": "9.0.0",
    "mocha": "7.1.1",
    "mochawesome": "5.0.0",
    "mochawesome-merge": "4.0.3",
    "mochawesome-report-generator": "4.1.0",
    "start-server-and-test": "^1.10.11"
  }
}
