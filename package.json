{
  "name": "cypress_test_framework",
  "version": "3.0.0",
  "private": true,
  "description": "Cypress test Framework",
  "scripts": {
    "lint": "tslint ./src/**/*.ts -t verbose",
    "combine-reports": "mochawesome-merge --reportDir cypress/reports/mocha > mochareports/report.json",
    "generate-report": "marge mochareports/*.json -f report -o mochareports",
    "cypress:open": "cypress open",
    "start:ci": "my-server -p 3030",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run --headed --spec 'cypress/integration/sample_spec.js'",
    "cy:azure": "cypress run --spec 'cypress/integration/example_spec.js'",
    "cy:ci": "cypress run --spec 'cypress/integration/example_spec.js'",
    "test:ci": "start-server-and-test start:ci http://localhost:3030 cy:run",
    "cypress:ci": "concurrently \"cypress run\" --kill-others \"ng serve\" --success first"
  },
  "keywords": [
    "node",
    "cypress",
    "test",
    "javascript"
  ],
  "author": "Dipjyoti Metia",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.7.1",
    "chalk": "2.4.2",
    "cross-env": "6.0.3",
    "cypress": "^3.5.0",
    "faker": "4.1.0",
    "lodash": "^4.17.15",
    "date-fns": "2.6.0",
    "uuid": "3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "concurrently": "^5.0.0",
    "cypress-dark": "^1.7.14",
    "cypress-file-upload": "^3.4.0",
    "cypress-multi-reporters": "1.2.3",
    "cypress-ntlm-auth": "^2.0.2",
    "cypress-plugin-retries": "^1.3.0",
    "cypress-xpath": "^1.4.0",
    "eslint-plugin-cypress": "^2.7.0",
    "mocha": "6.2.2",
    "mochawesome": "4.1.0",
    "mochawesome-merge": "2.0.1",
    "mochawesome-report-generator": "4.0.1",
    "start-server-and-test": "^1.10.6",
    "fs-extra": "8.1.0"
  },
  "repository": {}
}
