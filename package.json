{
  "name": "cypress_test_framework",
  "private": true,
  "description": "Cypress test Framework",
  "scripts": {
    "clean": "rimraf cypress/reports",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "combine-reports": "mochawesome-merge ./cypress/reports/mocha/*.json > mochareports/mochawesome.json",
    "generate-report": "marge mochareports/mochawesome.json",
    "cypress:open": "cypress open",
    "start:ci": "my-server -p 3030",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run --headed --spec 'cypress/integration/sample_spec.js'",
    "firefox:run": "cypress run --headed --browser firefox --spec 'cypress/integration/sample_spec.js'",
    "chrome:run": "cypress run --headed --browser chrome --spec 'cypress/integration/sample_spec.js'",
    "edge:run": "cypress run --headed --browser edge --spec 'cypress/integration/sample_spec.js'",
    "cy:ci": "cypress run --spec 'cypress/integration/example_spec.js'",
    "test:ci": "start-server-and-test start:ci http://localhost:3030 cy:run"
  },
  "keywords": [
    "node",
    "cypress",
    "test",
    "typescript"
  ],
  "author": "Dipjyoti Metia",
  "license": "MIT",
  "dependencies": {
    "axe-core": "^4.0.1",
    "chalk": "4.1.0",
    "cross-env": "7.0.2",
    "cypress": "^5.0.0",
    "cypress-axe": "^0.8.1",
    "cypress-downloadfile": "^1.2.0",
    "date-fns": "^2.16.1",
    "faker": "4.1.0",
    "lodash": "^4.17.20",
    "rimraf": "3.0.2",
    "uuid": "8.3.0",
    "typescript": "3.9.7"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/jquery": "3.5.1",
    "@types/mocha": "8.0.3",
    "@cypress/skip-test": "^2.5.0",
    "@neuralegion/cypress-har-generator": "^5.0.0",
    "@testing-library/cypress": "^6.0.0",
    "cypress-react-unit-test": "4.11.2",
    "concurrently": "^5.3.0",
    "cypress-dark": "^1.7.14",
    "cypress-file-upload": "^4.0.7",
    "cypress-multi-reporters": "1.4.0",
    "cypress-ntlm-auth": "^2.2.5",
    "cypress-xpath": "^1.6.0",
    "@percy/cypress": "^2.3.1",
    "eslint-plugin-cypress": "^2.11.1",
    "fs-extra": "9.0.1",
    "knex": "0.21.5",
    "mocha": "8.1.1",
    "mochawesome": "6.1.1",
    "mochawesome-merge": "4.1.0",
    "mochawesome-report-generator": "5.1.0",
    "start-server-and-test": "^1.11.3"
  }
}
